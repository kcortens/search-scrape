  "bugs": {
    "url": "https://github.com/robertkeizer/ddg-scrape/issues"
  },
  "homepage": "https://github.com/robertkeizer/ddg-scrape#readme",
  "devDependencies": {
    "assert": "^1.3.0",
    "casperjs": "^1.1.0-beta3",
    "istanbul": "^0.4.0",
    "jshint": "^2.8.0",
    "merge": "^1.2.0",
    "mocha": "^2.3.4",
    "spooky": "^0.2.5"
  }
}
